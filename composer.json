{
  "name": "xpressengine-plugin/react_board",
  "description": "This Package is Xpressengine Plugin - SPA Board.",
  "keywords": [
    "xpressengine",
    "plugin"
  ],
  "license": "LGPL-2.1",
  "version": "1.0.0",
  "type": "xpressengine-plugin",
  "support": {
    "email": "blueng.choi@gmail.com"
  },
  "authors": [
    {
      "name": "seungman choi",
      "email": "blueng.choi@gmail.com",
      "homepage": "http://blueng.tistory.com",
      "role": "Developer"
    }
  ],
  "extra": {
    "xpressengine": {
      "title": "SPA Board.",
      "component": {
        "module/react_board@board": {
          "class": "Xpressengine\\Plugins\\ReactBoard\\ReactBoardModule",
          "name": "SPA Board",
          "description": "기존 게시판 기능을 최소화하여 SPA로 구현한 게시판입니다.",
          "screenshot" : [
            "/plugins/react_board/assets/img/screenshots/menuType1.png"
          ]
        },
        "module/react_board@board/skin/react_board@default": {
          "class": "Xpressengine\\Plugins\\ReactBoard\\Skins\\DefaultSkin",
          "name": "SPA Board 기본 스킨",
          "description": "SPA Board 기본 스킨."
        },
         "module/react_board@board/settingsSkin/react_board@default": {
           "class": "Xpressengine\\Plugins\\ReactBoard\\Skins\\SettingsSkin",
           "name": "SPA Board 기본 스킨",
           "description": "SPA Board 기본 스킨."
        }
      }
    }
  },
  "require": {
  },
  "autoload": {
    "psr-4": {
      "Xpressengine\\Plugins\\ReactBoard\\": "src/"
    }
  }
}
